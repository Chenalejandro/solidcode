{
  "name": "turbocode",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "db:push": "pnpm --filter=website db:push",
    "db:seed": "pnpm --filter=website db:seed",
    "test:e2e": "pnpm --filter=website test:e2e",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "turbo": "^2.1.3",
    "typescript": "^5.6.3"
  },
  "packageManager": "pnpm@9.12.1",
  "engines": {
    "node": ">=20"
  },
  "pnpm": {
    "patchedDependencies": {
      "@stackframe/stack@2.6.5": "patches/@stackframe__stack@2.6.5.patch"
    }
  }
}
